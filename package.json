{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "e2e",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "1.5.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^7.0.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "prepare": "husky install"
  },
  "dependencies": {
    "react-undraw-illustrations": "^2.0.3",
    "tabler-icons-react": "^1.48.1",
    "web": "workspace:^"
  }
}
